<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    
    <parameters>
        
        <parameter key="kg_pager.class">KG\Bundle\PagerBundle\Pager\Pager</parameter>
        <parameter key="kg_pager.factory.generic.class">KG\Bundle\PagerBundle\Result\Provider\GenericFactory</parameter>
        <parameter key="kg_pager.interface.provider">KG\Bundle\PagerBundle\Result\Provider\ProviderInterface</parameter>
        
    </parameters>
    
    <services>
        
        <!-- Main paging service -->
        <service id="kg_pager" class="%kg_pager.class%" />
        
    </services>
</container>
